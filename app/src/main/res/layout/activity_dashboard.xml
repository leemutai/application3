<?xml version="1.0" encoding="UTF-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="dashboardVM" type="com.leemutaisapplication.app.modules.dashboard.data.viewmodel.DashboardVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnellipseone"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_24pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowletsfindyour"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnletsfindyour"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.84"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLetsFindyour"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{dashboardVM.dashboardModel.txtLetsFindyour}"
                        style="@style/txtPoppinsregular18" tools:text="@string/lbl_let_s_find_your"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFavoriteHome"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{dashboardVM.dashboardModel.txtFavoriteHome}"
                        style="@style/txtPoppinsbold20" tools:text="@string/lbl_favorite_home"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageEllipseOne"
                    android:layout_height="@dimen/_50pxh"
                    android:layout_width="@dimen/_50pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse1}"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_ellipse1"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linear10Dashboard"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_50">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowframe34220"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <androidx.appcompat.widget.SearchView
                    android:background="@drawable/rectangle_bg_white_a700_radius_10"
                    android:id="@+id/searchViewFrame34220"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.83"
                    android:layout_width="0dp"
                    android:theme="@style/searchViewFrame34220"
                    app:iconifiedByDefault="false"
                    app:queryBackground="@android:color/transparent"
                    app:queryHint="@string/msg_search_by_addre" app:searchIcon="@drawable/img_search"/>
                <ImageButton
                    android:background="@drawable/rectangle_bg_blue_a200_radius_10"
                    android:id="@+id/btnUser"
                    android:layout_height="@dimen/_52pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_width="@dimen/_52pxh"
                    android:paddingHorizontal="@dimen/_15pxh"
                    android:paddingVertical="@dimen/_15pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_user" tools:ignore="ContentDescription"/>
            </LinearLayout>
            <HorizontalScrollView android:fillViewport="true"
                android:gravity="center_horizontal"
                android:id="@+id/horizontalScrollHorizontalscrollgroupfiftyseven"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent" android:scrollbars="none">
                <LinearLayout android:id="@+id/linearRowgroupfiftyseven"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal" style="@style/groupStylecornerRadius">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGroupFiftySeven"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.29"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_9pxv"
                        android:paddingEnd="@dimen/_17pxh"
                        android:paddingStart="@dimen/_18pxh"
                        android:paddingTop="@dimen/_9pxv"
                        android:singleLine="true"
                        android:text="@{dashboardVM.dashboardModel.txtGroupFiftySeven}"
                        style="@style/txtSolidRounded" tools:text="@string/lbl_recomended"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGroupFiftyEight"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_weight="0.22"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_14pxh"
                        android:paddingStart="@dimen/_15pxh"
                        android:paddingTop="@dimen/_10pxv"
                        android:singleLine="true"
                        android:text="@{dashboardVM.dashboardModel.txtGroupFiftyEight}"
                        style="@style/txtSolidRounded_1" tools:text="@string/lbl_top_rates"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGroupFiftyNine"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_weight="0.24"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_10pxv"
                        android:paddingEnd="@dimen/_16pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_8pxv"
                        android:singleLine="true"
                        android:text="@{dashboardVM.dashboardModel.txtGroupFiftyNine}"
                        style="@style/txtSolidRounded_1" tools:text="@string/lbl_best_offers"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGroupSixty"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_weight="0.24"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_10pxv"
                        android:paddingEnd="@dimen/_6pxh"
                        android:paddingTop="@dimen/_8pxv"
                        android:singleLine="true"
                        android:text="@{dashboardVM.dashboardModel.txtGroupSixty}"
                        style="@style/txtSolidRounded_1" tools:text="@string/lbl_most_reated"/>
                </LinearLayout>
            </HorizontalScrollView>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="end"
                android:id="@+id/recyclerListrectanglefourteen"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_18pxh}" tools:itemCount="2" tools:listitem="@layout/row_listrectanglefourteen"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRownearyou"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNearYou"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.74"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{dashboardVM.dashboardModel.txtNearYou}"
                    style="@style/txtInterbold20" tools:text="@string/lbl_near_you"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMore"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_198pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.26"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{dashboardVM.dashboardModel.txtMore}"
                    style="@style/txtPoppinssemibold12" tools:text="@string/lbl_more"/>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerListrectangletwelve"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_10pxh}" tools:itemCount="2" tools:listitem="@layout/row_listrectangletwelve"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylewhite_A700">
            <LinearLayout android:id="@+id/linearSearch"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageGroupFiftyTwo"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_33pxh"
                    android:layout_marginStart="@dimen/_53pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group52"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group52"/>
                <ImageView android:id="@+id/imageGroupFifty"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_21pxh"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginEnd="@dimen/_53pxh"
                    android:layout_marginStart="@dimen/_65pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_183pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group50"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group50"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
